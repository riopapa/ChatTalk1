            android:layout_weight = "1"
            android:background = "@drawable/rounded_button3"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "ktTxtIg"
            android:text = "카톡 무시 글짜"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:layout_marginStart = "8dp"
        android:layout_marginEnd = "8dp"
        android:gravity = "center"
        android:orientation = "horizontal">

        <TextView
            android:id = "@+id/kt_no_number"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button3"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "ktNoNum"
            android:text = "카톡 숫자 무시"
            android:textSize = "@dimen/table_font_size"
