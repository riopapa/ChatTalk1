            android:text = "토스 글짜 무시"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />

        <TextView
            android:id = "@+id/package_names"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button3"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "appNames"
            android:text = "Package  Names"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />
    </LinearLayout>

    <TextView
        android:id = "@+id/tv_wifi_state"
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:layout_gravity = "center"
        android:layout_marginTop = "24dp"
        android:text = "WIFI"
        android:textStyle = "bold" />

</LinearLayout>