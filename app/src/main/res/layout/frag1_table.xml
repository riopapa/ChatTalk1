<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android = "http://schemas.android.com/apk/res/android"
      android:layout_width = "match_parent"
      android:layout_height = "match_parent"
      android:background="@color/color_tab_save_background"
      android:orientation = "vertical">

    <LinearLayout
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:layout_marginStart = "8dp"
        android:layout_marginEnd = "8dp"
        android:gravity = "center"
        android:orientation = "horizontal">

        <TextView
            android:id = "@+id/sms_who_ignores"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button3"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "smsWhoIg"
            android:text = "SMS에서 무시할 사람"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />

        <TextView
            android:id = "@+id/sms_text_ignores"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button3"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "smsTxtIg"
            android:text = "SMS에 이 글짜면 제외"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:layout_marginStart = "8dp"
        android:layout_marginEnd = "8dp"
        android:gravity = "center"
        android:orientation = "horizontal">

        <TextView
            android:id = "@+id/sms_with_no_number"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button3"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "smsNoNum"
            android:text = "SMS 숫자 무시"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />

        <TextView
            android:id = "@+id/for_future1"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button1"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "xxxx"
