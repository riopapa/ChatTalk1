            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:layout_marginStart = "8dp"
        android:layout_marginEnd = "8dp"
        android:gravity = "center"
        android:orientation = "horizontal">

        <TextView
            android:id = "@+id/system_ignores"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
            android:layout_weight = "1"
            android:background = "@drawable/rounded_button1"
            android:gravity = "center"
            android:padding = "@dimen/table_padding_size"
            android:tag = "sysIg"
            android:text = "무시할 시스템 메시지"
            android:textSize = "@dimen/table_font_size"
            android:textStyle = "bold" />

        <TextView
            android:id = "@+id/app_text_ignores"
            android:layout_width = "0dp"
            android:layout_height = "wrap_content"
            android:layout_margin = "@dimen/button_margin_size"
